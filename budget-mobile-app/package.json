{
  "name": "budget-mobile-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:optimized": "vite --mode optimized",
    "build": "vite build",
    "build:optimized": "vite build --mode optimized",
    "build:production": "vite build --config vite.config.production.js",
    "build:deploy": "vite build --mode production --config vite.config.production.js",
    "build:analyze": "vite build --mode analyze",
    "test": "vitest",
    "test:performance": "vitest --run performance",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "preview": "vite preview",
    "start": "node server/api.js",
    "start:optimized": "node server/api.js --optimized",
    "performance:test": "node scripts/performance-test.js",
    "performance:analyze": "node scripts/analyze-performance.js",
    "optimize": "npm run build:optimized && npm run performance:test",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@react-oauth/google": "^0.12.2",
    "@stripe/stripe-js": "^2.4.0",
    "@stripe/react-stripe-js": "^2.4.0",
    "@tailwindcss/postcss": "^4.1.8",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^10.1.0",
    "helmet": "^7.1.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "jwt-decode": "^4.0.0",
    "mongodb": "^6.5.0",
    "node-cron": "^4.1.0",
    "nprogress": "^0.2.0",
    "postcss": "^8.5.4",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.5.0",
    "react-joyride": "^2.9.3",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^6.20.1",
    "react-swipeable": "^7.0.2",
    "react-window": "^1.8.11",
    "stripe": "^14.25.0",
    "tailwindcss": "^3.4.17",
    "web-push": "^3.6.7",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.1.6",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.17.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
